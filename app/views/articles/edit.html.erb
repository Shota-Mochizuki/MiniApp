<div class="contents row">
    <%= form_for @article do |f| %>
      <h3>編集する</h3>
      <div class="field">
        <%= f.label :text %><br />
        <%= f.text_area :text, autofocus: true, autocomplete: @article.text, rows: 10, cols: 30 %>
      </div>
      <div class="actions">
        <%= f.submit "SEND" %>
      </div>
    <% end %>
  </div>